{
    "version": 2,
    "builds": [
        {
            "src": "Server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/hoi",
            "dest": "/src/Doc/Swagger/master.swagger.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/Server.ts"
        }
    ]
}